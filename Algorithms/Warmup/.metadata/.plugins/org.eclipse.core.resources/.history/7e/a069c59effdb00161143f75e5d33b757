using namespace std;

#include <iostream>

void Compare(struct triplet T1, struct triplet T2)
{
	int A=0, B=0;
	if(T1.t1 > T1.t2)
		++A;
}

void getScore(struct triplet* T)
{
	cin >> T->t1;
	cin >> T->t2;
	cin >> T->t3;
}

struct triplet
{
	int t1, t2, t3;
};

int main()
{
	struct triplet A, B;
	getScore(&A);
	getScore(&B);

	Compare(A,B);

	return 0;
}
