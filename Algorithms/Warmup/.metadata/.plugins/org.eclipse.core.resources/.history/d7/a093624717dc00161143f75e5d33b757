using namespace std;

#include <iostream>
#include <sstream>
#include <vector>

vector<string> split(stringstream ss)
{
	string s;
	vector<string> str;

	while(getline(ss, s, ':'))
	{
		str
	}
}

int main()
{
	string time;
	cin >> time;

	stringstream ss(time);

	vector<string> military = split(ss);

	return 0;
}
