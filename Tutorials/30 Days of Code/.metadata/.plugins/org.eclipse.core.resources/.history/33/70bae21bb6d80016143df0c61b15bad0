using namespace std;

#include <iostream>


void Initialize(int a[][6])
{
	for(int i=0;i<6;++i)
		for(int j=0;j<6;++j)
			cin >> a[i][j];
}

int main()
{
	int array[6][6];
	Initialize(array);

	cout << array[3][2];

	return 0;
}
