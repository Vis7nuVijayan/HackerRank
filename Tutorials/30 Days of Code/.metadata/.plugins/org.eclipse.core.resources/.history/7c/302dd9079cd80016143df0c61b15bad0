using namespace std;

#include <iostream>
#include <algorithm>

string getBinary(int n)
{
	string s;
	while(n)
	{
		s += (n&1+'0');
		n = n >> 1;
	}

	reverse(s.begin(), s.end());

	return s;
}

int main()
{
	int n;
	cin >> n;

	string binary = getBinary(n);

	return 0;
}
